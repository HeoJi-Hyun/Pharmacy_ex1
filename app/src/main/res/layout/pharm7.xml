<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv1Pharm6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@drawable/hyper2"
            android:gravity="center_horizontal"
            android:text="메디팜사랑약국"
            android:textColor="@color/pharmacy"
            android:textSize="35dp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="match_parent"
            android:layout_height="304dp"
            android:layout_marginLeft="10sp"
            android:layout_marginRight="10sp"
            android:src="@drawable/lovepharm7" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/hyper2" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/image2Pharm6"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginRight="10dp"
                android:src="@drawable/img" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tv11Pharm6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="도로명 : 서울 동대문구 망우로 16길 6, 1층"
                    android:textColor="#000000"
                    android:textColorHint="#989898"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/tv2Pharm6"
                    android:layout_width="320dp"
                    android:layout_height="wrap_content"

                    android:text="지번 : 휘경동 276-57"
                    android:textColor="#000000"
                    android:textSize="20sp" />

            </LinearLayout>
        </LinearLayout>

        <TextView
            android:id="@+id/tv3Pharm6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/hyper"
            android:textColor="#000000"
            android:textColorHint="#D6D6D6"
            android:textSize="5dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/image3Pharm6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:src="@drawable/img_1" />

            <TextView
                android:id="@+id/tv4Pharm6"
                android:layout_width="320dp"
                android:layout_height="wrap_content"
                android:text="02-2213-5005"
                android:textColor="#000000"
                android:textSize="20sp" />
        </LinearLayout>

        <TextView
            android:id="@+id/tv5Pharm6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/hyper"
            android:textColor="#000000"
            android:textSize="3dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/image4Pharm3"
                android:layout_width="30dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="3dp"
                android:layout_marginRight="10dp"
                android:src="@drawable/img_2" />

            <TextView
                android:id="@+id/tv6Pharm3"
                android:layout_width="320dp"
                android:layout_height="wrap_content"
                android:text="제로페이"
                android:textColor="#000000"
                android:textSize="20sp" />

        </LinearLayout>

        <TextView
            android:id="@+id/tv7Pharm6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/hyper"
            android:textSize="5dp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/image5Pharm3"
                android:layout_width="30dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="2dp"
                android:layout_marginRight="10dp"
                android:src="@drawable/img_3" />

            <TextView
                android:id="@+id/tv8Pharm6"
                android:layout_width="320dp"
                android:layout_height="wrap_content"
                android:text="이용시간 : 월~목 09:00 - 18:00"
                android:textColor="#000000"
                android:textSize="20sp" />

        </LinearLayout>

        <TextView
            android:id="@+id/tv10Pharm"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="                                 금 09:00 - 14:00"
            android:textColor="#000000"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv10Pharm3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="                                 일 09:00 - 18:00"
            android:textColor="#000000"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/tv9Pharm3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/hyper"
            android:textColor="#000000"
            android:textSize="5dp" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>