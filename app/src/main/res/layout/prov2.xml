<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <TextView
        android:id="@+id/tvProv21"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="탁센"
        android:textColor="@color/pharmacy"
        android:textSize="40dp"
        android:textStyle="bold" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="130dp"
        android:layout_marginLeft="10dp">

        <ImageView
            android:id="@+id/imageProv21"
            android:layout_width="207dp"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/xkrxkr" />

        <ImageView
            android:id="@+id/imageProv22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/medi3" />
    </LinearLayout>

    <com.google.android.material.tabs.TabLayout
        android:id="@+id/tabs2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/background_light"
        android:elevation="1dp"
        android:scrollbarSize="3dp"
        app:tabGravity="fill"
        app:tabIndicatorColor="@color/pharmacy"
        app:tabMode="fixed"
        app:tabSelectedTextColor="#3C3C3C"
        app:tabTextColor="#3C3C3C"
        tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />

    <FrameLayout
        android:id="@+id/container2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/tabs2">

    </FrameLayout>


</LinearLayout>